package com.mitchellbosecke.pebble.template;

import org.checkerframework.checker.nullness.qual.Nullable;

public interface PebbleTemplate {
  void evaluate(Writer writer) throws IOException;
  void evaluate(Writer writer, Locale locale) throws IOException;
  void evaluate(Writer writer, Map<@Nullable String, @Nullable Object> context) throws IOException;
  void evaluate(Writer writer, Map<@Nullable String, @Nullable Object> context, Locale locale) throws IOException;
}
